	<bean id="baidusiteDataFetcher" class="com.eadmarket.pangu.api.website.impl.BaidusiteDataFetcher">
		<property name="charset" value="utf-8" />
		<property name="serverUrl" value="http://www.baidu.com/s?wd=site%3A" />
	</bean>
	
	<bean id="baiduTodayDataFetcher" class="com.eadmarket.pangu.api.website.impl.BaiduTodayDataFetcher">
		<property name="charset" value="utf-8" />
		<property name="serverUrl" value="http://www.baidu.com/s?wd=site%3A" />
	</bean>
	
	<bean id="baidulinkDataFetcher" class="com.eadmarket.pangu.api.website.impl.BaidulinkDataFetcher">
