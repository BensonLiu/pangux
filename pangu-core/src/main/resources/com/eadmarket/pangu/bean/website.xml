	<bean id="googlesiteDataFetcher" class="com.eadmarket.pangu.api.website.impl.GooglesiteDataFetcher">
		<property name="charset" value="utf-8" />
		<property name="serverUrl" value="http://www.google.com.hk/search?q=site%3A" />
